{
  "name": "lookpin-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "node-sass": "^4.5.3",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.11",
    "redux": "^3.7.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/styles -o src/styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles -o src/styles/css --watch --recursive",
    "start-js": "cross-env NODE_PATH=src react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "yarn build-css && cross-env NODE_PATH=src react-scripts build",
    "test": "cross-env NODE_PATH=src react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^5.0.5",
    "enzyme": "^2.9.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "moxios": "^0.4.0",
    "prettier": "^1.5.3",
    "react-test-renderer": "^15.6.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.2.0"
  }
}
